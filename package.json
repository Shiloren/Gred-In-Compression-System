{
    "name": "@gredinlabstechnologies/gics-core",
    "version": "1.3.2",
    "description": "GICS core compression library (generic)",
    "type": "module",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shiloren/Gred-In-Compression-System.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:daemon:critical": "vitest run tests/daemon-memtable.test.ts tests/daemon-recovery.test.ts tests/daemon-wal.test.ts tests/daemon-file-lock.test.ts tests/daemon-realworld-soak.test.ts",
        "quality:compression": "node bench/scripts/validate-compression-report.mjs",
        "quality:check": "npm run test:daemon:critical && npm run test:coverage && npm run bench:forensics:verify && npm run quality:compression",
        "quality:sonar:prep": "npm run test:coverage && npm run bench:forensics:verify && npm run quality:compression",
        "prepublishOnly": "npm run build",
        "bench": "tsx bench/scripts/harness.ts && tsx bench/scripts/gen-report.ts",
        "bench:empirical": "tsx bench/scripts/empirical.ts",
        "bench:gate": "tsx bench/scripts/empirical.ts",
        "bench:validate-50x": "tsx bench/scripts/validate-50x-guarantee.ts",
        "bench:security": "tsx bench/scripts/empirical-security.ts",
        "bench:edge-cases": "tsx bench/scripts/empirical-edge-cases.ts",
        "bench:codec-stats": "tsx bench/scripts/empirical-codec-stats.ts",
        "bench:forensics": "tsx bench/forensics/postfreeze/harness.postfreeze.ts",
        "bench:forensics:verify": "tsx bench/forensics/postfreeze/verifier.postfreeze.ts",
        "bench:forensics:summary": "tsx bench/forensics/postfreeze/summary.postfreeze.ts",
        "verify": "tsx tools/verify/verify.ts",
        "profile": "tsx tools/profile.ts",
        "sonar": "sonar-scanner",
        "lint": "eslint src/**/*.ts",
        "bench:strict": "tsx bench/scripts/empirical-strict.ts",
        "bench:all": "npm run bench:empirical && npm run bench:strict && npm run bench:validate-50x && npm run bench:security && npm run bench:edge-cases && npm run bench:codec-stats",
        "quality:strict": "npm run bench:gate && npm run bench:validate-50x",
        "quality:strict:full": "npm run quality:strict && npm run bench:strict && npm run bench:security && npm run bench:edge-cases && npm run bench:codec-stats"
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.1.3",
        "apache-arrow": "^21.1.0",
        "protobufjs": "^8.0.0",
        "zstd-codec": "^0.1.5"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^4.0.18",
        "@eslint/js": "^9.39.2",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "eslint": "^9.39.2",
        "eslint-plugin-sonarjs": "^3.0.6",
        "globals": "^17.3.0",
        "sonarqube-scanner": "^4.3.4",
        "tsx": "^4.19.2",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.55.0",
        "vitest": "^4.0.18"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}